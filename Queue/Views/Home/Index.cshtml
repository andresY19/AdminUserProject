                        borderWidth: 1
                    }]
                };
                if (ChartImproductive != null) {
                    ChartImproductive.destroy();
                }
                var ctx = $("#chartImproductive").get(0).getContext("2d");
                ChartImproductive = new Chart(ctx, {
                    type: 'bar',
